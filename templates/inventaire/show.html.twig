{% extends 'base.html.twig' %}

{% block title %}Inventaire{% endblock %}

{% block body %}
    <h1>Inventaire</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ inventaire.id }}</td>
            </tr>
            <tr>
                <th>Update_at</th>
                <td>{{ inventaire.updateAt ? inventaire.updateAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ inventaire.note }}</td>
            </tr>
            <tr>
                <th>Stockinventaire</th>
                <td>{{ inventaire.stockinventaire }}</td>
            </tr>
            <tr>
                <th>Stockutiliser</th>
                <td>{{ inventaire.stockutiliser }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_inventaire_index') }}">back to list</a>

    <a href="{{ path('app_inventaire_edit', {'id': inventaire.id}) }}">edit</a>

    {{ include('inventaire/_delete_form.html.twig') }}
{% endblock %}
